{
  "name": "@capacitor-community/http",
  "version": "1.0.0-alpha.0",
  "description": "A native HTTP plugin for CORS-free requests and file transfers",
  "main": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && rollup -c rollup.config.js",
    "clean": "rimraf ./dist",
    "watch": "tsc --watch",
    "fmt": "npm run prettier -- --write",
    "prettier": "prettier \"**/*.{css,html,ts,js,java}\"",
    "prepublishOnly": "npm run build",
    "npm-publish": "np"
  },
  "author": "Max Lynch <max@ionic.io>",
  "license": "MIT",
  "dependencies": {
    "@capacitor/android": "^3.0.0-beta.2",
    "@capacitor/core": "^3.0.0-beta.2",
    "@capacitor/ios": "^3.0.0-beta.2",
    "@capacitor/filesystem": "^0.3.1"
  },
  "devDependencies": {
    "@ionic/prettier-config": "^1.0.1",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "husky": "^4.3.7",
    "multer": "^1.4.2",
    "np": "^7.2.0",
    "prettier": "^2.2.1",
    "prettier-plugin-java": "^1.0.1",
    "pretty-quick": "^3.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.35.1",
    "typescript": "^4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  },
  "files": [
    "dist/",
    "ios/",
    "android/",
    "CapacitorCommunityHttp.podspec"
  ],
  "keywords": [
    "capacitor",
    "plugin",
    "native"
  ],
  "prettier": "@ionic/prettier-config",
  "capacitor": {
    "ios": {
      "src": "ios"
    },
    "android": {
      "src": "android"
    }
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/capacitor-community/http"
  },
  "bugs": {
    "url": "https://github.com/capacitor-community/http/issues"
  }
}
